<h1>Welcome to New Missions</h1>

<%= form_with model: @mission, local: true do |f| %>

    <%=f.label "Company Name"%>
    <%=f.text_field :company_id%><br>

    <%=f.label "Dock Name"%>
    <%=f.text_field :dock_id%><br>

    <%=f.label "Ship"%>
    <%=f.text_field :ship%><br>
    
    <%=f.submit "Start Mission"%>
<%end%>