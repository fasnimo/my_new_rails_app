<%= form_with model: @mission, local: true do |f| %>
    <%= f.label :company_id %>
    <%= f.number_field :company_id %>

    <%= f.label :dock_id %>
    <%= f.number_field :dock_id %>

    <%=f.label :ship, "Ship Info"%><br>
    <%=f.text_area :ship%><br>
    
    <%=f.submit "Mission Info"%>
<%end%>