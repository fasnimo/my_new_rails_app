<h3>Hello <%=@company.name%></h3>



<%= form_with model: @mission, local: true do |f| %>
    
    <%= f.hidden_field :company_id, value: session[:company_id] %>

    

    <%= f.label :title, "Port Name"%>
    <%= f.collection_select :title, Port.all, :id, :title %><br>

    <%= f.label :item, "Item"%>
    <%= f.collection_select :item, Port.all, :id, :item %><br>

    <%=f.label :ship, "Ship Info"%><br>
    <%=f.text_area :ship%><br>
    
    <%=f.submit%>
<%end%>