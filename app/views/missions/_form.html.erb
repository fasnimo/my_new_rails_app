<h3>Hello <%=@company.name%></h3>

<%= form_with model: @mission, local: true do |f| %>
    
    <%= f.hidden_field :company_id, value: session[:company_id] %>

    <%= f.hidden_field :dock_id, value: session[:dock_id]%>

    <%= f.label :title, "Dock Name"%>
    <%= f.collection_select :title, Dock.all, :id, :title %><br>

    <%= f.label :item, "Item"%>
    <%= f.collection_select :item, Dock.all, :id, :item %><br>

    <%=f.label :ship, "Ship Info"%><br>
    <%=f.text_area :ship%><br>
    
    <%=f.submit "Mission Info"%>
<%end%>