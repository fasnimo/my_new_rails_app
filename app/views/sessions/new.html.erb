<h1>Welcome to Sessions New</h1>


<%=form_with model: @company, local: true do |f|%>
    <%= f.label :name %>
    <%=f.text_field :name%><br><br>

    <%=f.label :ship_name%>
    <%=f.text_field :ship_name%><br><br>

    <%=f.label :password, 'Password'%>
    <%=f.password_field :password%><br><br>

    <%=f.submit%>

<%end%>